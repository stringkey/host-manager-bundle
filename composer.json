{
  "name": "stringkey/host-manager-bundle",
  "type": "symfony-bundle",
  "description": "helpers to manage server and service hosts within a domain",
  "keywords": ["php", "symfony", "hosts", "managment tools"],
  "license": "MIT",
  "authors": [
    {
      "name": "Erik van de Laar",
      "email": "stringkey@wxs.nl"
    }
  ],
  "autoload": {
    "psr-4": { "Stringkey\\HostManagerBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      "Stringkey\\HostManagerBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "symfony/dependency-injection": "^7.0"
  },
  "require-dev": {
    "symfony/http-kernel": "^7.0",
    "phpunit/phpunit": "^11.1",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.9",
    "friendsofphp/php-cs-fixer": "^3.53"
  }
}
